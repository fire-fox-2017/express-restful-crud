<!DOCTYPE html>
<html>
  <head>
    <title>CRUD</title>
  </head>
<body>
  <a href="/users/add_user">Add Users</a>
  <table>
  <tr>
    <th>No</th>
    <th>Name</th>
    <th>Email</th>
    <th>Action</th>
  </tr>
  <% let i=1; tampil.forEach(user=>{ %>
  <tr>
    <td><%= i++ %></td>
    <td><%= user.fullname %></td>
    <td><%= user.email %></td>
    <td><a href="/users/delete/<%= user.id %>">Delete</a> <a href="/users/edit/<%= user.id %>">Edit</a></td>
  </tr>
  <% }) %>
  </table>
</body>
</html>
